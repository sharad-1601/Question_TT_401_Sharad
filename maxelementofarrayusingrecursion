#include <iostream>
using namespace std;

int findMax(int arr[], int size)
{
    int maxElement = arr[0]; 


    for (int i = 1; i < size; i++) {
        if (arr[i] > maxElement) {
            maxElement = arr[i];
        }
    }

    return maxElement;
}

int main()
{
    int n;

    cin >> n;

    int arr[n];
    
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    int maxElement = findMax(arr, n);
    
    cout << maxElement;
  

    return 0;
}
